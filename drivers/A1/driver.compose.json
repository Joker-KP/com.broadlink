{
	"id": "A1",
	"name": {
		"en": "A1",
		"nl": "A1"
	},
	"images": {
		"large": "{{driverAssetsPath}}/images/large.jpg",
		"small": "{{driverAssetsPath}}/images/small.jpg"
	},
	"class": "sensor",
	"capabilities": [
		"updateSensor",
		"measure_temperature",
		"measure_humidity",
		"a1_light_level",
		"a1_air_quality",
		"a1_noise_level",
		"a1_air_quality_number",
		"a1_light_level_number",
		"a1_noise_level_number"
	],
	"mobile": {
		"components": [
			{
				"id": "icon",
				"capabilities": [
					"updateSensor"
				]
			},
			{
				"id": "sensor",
				"capabilities": [
					"a1_air_quality",
					"a1_noise_level",
					"a1_light_level",
					"measure_temperature",
					"measure_humidity"
				],
				"options": {
					"icons": {
						"a1_air_quality": "/assets/airquality.svg",
						"a1_noise_level": "/assets/noiselevel.svg",
						"a1_light_level": "/assets/lightlevel.svg"
					}
				}
			}
		]
	},
	"pair": [
		{
			"$template": "start",
			"id": "start"
		},
		{
			"id": "list_devices",
			"template": "list_devices",
			"navigation": {
				"next": "add_devices"
			}
		},
		{
			"id": "add_devices",
			"template": "add_devices"
		}
	],
